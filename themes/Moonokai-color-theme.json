{
  "name": "Moonokai",
  "type": "dark",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#5F7A87"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#FF2C96"
      }
    },
    {
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#B78AFF"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#B78AFF"
      }
    },
    {
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#B78AFF"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F8F8F2",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF2C96"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#FF2C96",
        "fontStyle": ""
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#2EDCFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type, entity.name.class",
      "settings": {
        "foreground": "#9DFF00",
        "fontStyle": "underline"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#9DFF00",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#9DFF00",
        "fontStyle": ""
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FD971F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF2C96",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#9DFF00",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#2EDCFF",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#2EDCFF",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#2EDCFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0",
        "fontStyle": ""
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "#CFCFC2"
      }
    },
    {
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FF2C96"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#9DFF00"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#B78AFFA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FF2C96"
      }
    },
    {
      "scope": "markup.list",
      "settings": {
        "foreground": "#E6DB74"
      }
    },
    {
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "#2EDCFF"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#FD971F",
        "fontStyle": ""
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#9DFF00"
      }
    },
    {
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#9DFF00",
        "fontStyle": ""
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "#FD971F"
      }
    }
  ]
}
